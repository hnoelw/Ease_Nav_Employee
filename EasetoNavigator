  spreadsheet.getRange('Paste_Ease_Data!F2:F').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);

  //paste relationship
  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('Copy_For_Navigator'), true);
  spreadsheet.getRange('C2:C').activate();
  spreadsheet.getRange('Paste_Ease_Data!G2:G').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);

