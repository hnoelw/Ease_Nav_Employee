function EasetoNavigator() {
  var spreadsheet = SpreadsheetApp.getActive();

  //Paste first name
  spreadsheet.getRange('D2:D').activate;
  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('Copy_For_Navigator'), true);
  spreadsheet.getRange('E2:E').activate();
  spreadsheet.getRange('Paste_Ease_Data!D2:D').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);

  //paste middle name
  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('Copy_For_Navigator'), true);
  spreadsheet.getRange('F2:F').activate();
  spreadsheet.getRange('Paste_Ease_Data!E2:E').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);

  //paste last name
  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('Copy_For_Navigator'), true);
